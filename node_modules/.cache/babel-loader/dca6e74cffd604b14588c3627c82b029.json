) {\n        this.invalidInput = true;\n        return;\n      }\n      this.invalidInput = false;\n\n      // this.$emit('survey-submit', {\n      //   userName: this.enteredName,\n      //   rating: this.chosenRating,\n      // });\n    this.error = null\n      fetch('https://vue-http-aea9f-default-rtdb.firebaseio.com/surveys.json', {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: JSON.stringify({\n          name: this.enteredName,\n          rating: this.chosenRating\n        })\n      }).then(response => {\n        if(response.ok){\n          //\n        }else{\n          throw new Error('Could not save data')\n        }\n      }).catch((error) => {\n        console.log(error)\n        this.error = 'Something went wrong!'\n      })\n      this.enteredName = '';\n      this.chosenRating = null;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.form-control {\n  margin: 0.5rem 0;\n}\n\ninput[type='text'] {\n  display: block;\n  width: 20rem;\n  margin-top: 0.5rem;\n}\n</style>"],"sourceRoot":""}]}